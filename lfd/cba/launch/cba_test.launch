<launch>
	<param name="/cba/max_data_points" value="64" />
	<param name="/cba/dist_thresh_mult" value="1.75" />
	
	<!-- Create an instance of each appropriate node -->
	<node name="cba" pkg="cba" type="cba" respawn="false" output="screen"/>
  	<node name="mock_classify" pkg="cba" type="mock_classify" respawn="false" output="screen"/>
  	<node name="mock_state_publisher" pkg="cba" type="mock_state_publisher" respawn="false" output="screen"/>
</launch>

